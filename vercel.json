{
  "framework": null,
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/video/:videoId",
      "destination": "/index.html"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "functions": {
    "api/analyze.ts": {
      "runtime": "@vercel/node"
    },
    "api/video/[videoId].ts": {
      "runtime": "@vercel/node"
    }
  }
}
