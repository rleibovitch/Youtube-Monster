{
  "framework": null,
  "outputDirectory": "public",
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.1.5"
    },
    "api/transcribe.py": {
      "runtime": "python@3.11"
    },
    "api/analyze-asr.py": {
      "runtime": "python@3.11"
    }
  },
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/video/:videoId",
      "destination": "/index.html"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}
